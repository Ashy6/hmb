<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<!--mapper的命名空间: 规定指定mybatis的映射xml文件-->
<!--        mybatis中dao接口与mapper配置文件在做映射绑定-->
<mapper namespace="com.hellomybody.hmb.dao.Dao_cp_immunity">
    <!--    增加-->
    <insert id="addMyl">
        INSERT INTO z_immunity
        (name,type1,type2,type3,type4,resource5,type6,resource7,resource8,resource9,resource10,resource11,resource12,type13,timesss,event)
        VALUE
        (#{name},#{type1},#{type2},#{type3},#{type4},#{resource5},#{type6},#{resource7},#{resource8},#{resource9},#{resource10},#{resource11},#{resource12},#{type13},#{timesss
        },#{event})
    </insert>
</mapper>